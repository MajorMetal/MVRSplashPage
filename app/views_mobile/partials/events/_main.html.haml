.container.events#events{role: "tablist", aria: {multiselectable: "false"}}
  .row.event
    .event__header#hack-n-haunt{role: "tab"}
      .col-sm-4
        %h4 Hack n Haunt Hackathon
        %p.subheader Sep 9th, 2016

    .event__body.collapse.in#hack-n-haunt-body{role: "tabpanel", aria: {labelledby: "hack-n-haunt"}}
      .col-sm-4
        %p.subheader Location: TBD
        %p.event__details Hey guys! We are throwing a horror ‪#‎hackathon‬ w/ Titmouse Animation Sep. 9th! ‪#‎vr‬

    %a.btn{href: "http://ow.ly/ZubR302McJh"} Apply Here

    .event__controls{data: {toggle: "collapse", parent: "#events", target: "#hack-n-haunt-body"}, aria: {expanded: "true", controls: "hack-n-haunt-body"}}
